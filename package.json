{
  "name": "roborail",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "bun run next dev --turbopack",
    "build": "bun run next build",
    "start": "bun run next start",
    "lint": "bun run next lint",
    "type-check": "bun run tsc --noEmit",
    "test": "bun test --preload ./tests/setup.bun.ts --timeout 5000",
    "test:ui": "bun test --preload ./tests/setup.bun.ts --watch",
    "test:coverage": "bun test --preload ./tests/setup.bun.ts --coverage",
    "test:watch": "bun test --preload ./tests/setup.bun.ts --watch",
    "test:e2e": "bun run playwright test",
    "test:e2e:ui": "bun run playwright test --ui",
    "test:e2e:debug": "bun run playwright test --debug",
    "test:e2e:headed": "bun run playwright test --headed",
    "test:unit": "vitest --config=tests/vitest.config.unit.ts",
    "test:unit:watch": "vitest --config=tests/vitest.config.unit.ts --watch",
    "test:unit:coverage": "vitest --config=tests/vitest.config.unit.ts --coverage",
    "test:integration": "vitest --config=tests/vitest.config.integration.ts",
    "test:integration:watch": "vitest --config=tests/vitest.config.integration.ts --watch",
    "test:integration:coverage": "vitest --config=tests/vitest.config.integration.ts --coverage",
    "test:acceptance": "vitest --config=tests/vitest.config.integration.ts tests/acceptance/",
    "test:all": "./tests/scripts/run-tests.sh all",
    "test:tdd": "./tests/scripts/run-tests.sh all",
    "test:ci": "SKIP_E2E=true ./tests/scripts/run-tests.sh all",
    "playwright:install": "bun run playwright install",
    "playwright:codegen": "bun run playwright codegen",
    "install:clean": "rm -rf node_modules bun.lockb && bun install",
    "postinstall": "bun run playwright install"
  },
