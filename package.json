{
  "name": "roborail-assistant-chat",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:clean": "pnpm run clean && next dev --turbopack",
    "build": "next build",
    "build:clean": "pnpm run clean && next build",
    "start": "next start",
    "clean": "rm -rf .next && rm -rf .turbo",
    "clean:cache": "rm -rf .next/cache && rm -rf .turbo",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write \"app/**/*.{ts,tsx,css}\" \"components/**/*.{ts,tsx,css}\" \"lib/**/*.{ts,tsx,css}\"",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "test:unit": "TEST_TYPE=unit vitest run",
    "test:unit:watch": "TEST_TYPE=unit vitest --watch",
    "test:integration": "TEST_TYPE=integration vitest run",
    "test:integration:watch": "TEST_TYPE=integration vitest --watch",
    "test:coverage": "COVERAGE=1 vitest run --coverage",
    "test:coverage:watch": "COVERAGE=1 vitest --coverage --watch",
    "test:coverage:ui": "COVERAGE=1 vitest --coverage --ui",
    "test:coverage:ci": "CI=1 COVERAGE=1 vitest run --coverage --reporter=verbose --reporter=json --outputFile=test-results.json",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:fast": "TEST_TYPE=fast vitest run",
    "test:fast:watch": "TEST_TYPE=fast vitest --watch",
    "test:isolated-gateway": "vitest run tests/isolated/ai-gateway-isolated.test.ts",
    "test:gateway-live": "npx tsx tests/isolated/gateway-live-test.ts",
    "test:ai-sdk-gateway": "npx tsx tests/isolated/ai-sdk-gateway-test.ts",
    "test:chat-gateway": "npx tsx tests/isolated/chat-gateway-test.ts",
    "debug:gateway-routing": "npx tsx tests/isolated/debug-gateway-routing.ts",
    "test:openai-compat-gateway": "npx tsx tests/isolated/openai-compat-gateway-test.ts",
    "test:gpt5-gateway": "npx tsx tests/isolated/test-gpt5-gateway.ts",
    "test:responses-api-gateway": "npx tsx tests/isolated/test-responses-api-gateway.ts",
    "test:vercel-integration": "npx tsx tests/isolated/vercel-integration-test.ts",
    "test:safe": "TEST_TYPE=fast vitest run --reporter=verbose --pool=threads --poolOptions.threads.singleThread=true",
    "setup:test-env": "npx tsx tests/isolated/setup-test-env.ts",
    "test:validate-quality": "npx tsx tests/validation/test-quality-validator.ts",
    "test:production-readiness": "npx tsx tests/scripts/validate-production-readiness.ts",
    "playwright:install": "playwright install",
    "test:file-search": "tsx tests/scripts/test-native-file-search.ts",
    "setup:vector-store": "tsx tests/scripts/setup-vector-stores.ts",
    "setup:vector-store:list": "tsx tests/scripts/setup-vector-stores.ts list",
    "setup:vector-store:create": "tsx tests/scripts/setup-vector-stores.ts create",
    "setup:vector-store:setup": "tsx tests/scripts/setup-vector-stores.ts setup",
    "setup:vector-store:test-upload": "tsx tests/scripts/setup-vector-stores.ts test-upload"
  },
  "packageManager": "pnpm@9.0.0",
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.17",
    "@ai-sdk/google": "^2.0.14",
    "@ai-sdk/mistral": "^2.0.14",
    "@ai-sdk/openai": "^2.0.32",
    "@ai-sdk/perplexity": "^2.0.9",
    "@ai-sdk/react": "^2.0.46",
    "@ai-sdk/ui-utils": "^1.2.11",
    "@ai-sdk/xai": "^2.0.20",
    "@anthropic-ai/sdk": "^0.63.0",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@radix-ui/react-alert-dialog": "1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
